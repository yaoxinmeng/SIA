<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.xinmeng.sia.SupervisorPriorityTask">

    <fragment
        android:id="@+id/bgfragmentsup"
        android:name="com.example.xinmeng.sia.SkyOrangeBgFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:layout="@layout/skyorangebg"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp" />

    <TextView
        android:id="@+id/taskSubmission"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Priority Task Submission"
        android:textSize="26sp"
        android:textColor="#000000"
        android:gravity="center"
        tools:layout_editor_absoluteY="33dp"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="272dp"
        android:layout_height="9dp"
        app:srcCompat="@drawable/suppriority_blueline"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="270dp"
        android:layout_height="52dp"
        android:text="For Manual Assignment, please enter tech ID"
        android:textSize="22sp"
        android:gravity="center"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="93dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="1.6in"
        android:layout_height="0.35in"
        android:background="@drawable/suppriority_finishbutton"
        tools:layout_editor_absoluteY="495dp"
        tools:layout_editor_absoluteX="112dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="0.3in"
        android:layout_height="0.3in"
        android:background="@drawable/suptask_leftbutton"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="495dp" />

    <EditText
        android:id="@+id/techIDField"
        android:layout_width="1.7in"
        android:layout_height="0.22in"
        android:background="@drawable/suppriority_inputfield"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="165dp" />

    <TextView
        android:id="@+id/techLeft"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="250dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0.12in"
        android:layout_height="0.12in"
        app:srcCompat="@drawable/suppriority_asterisk"
        tools:layout_editor_absoluteY="250dp"
        tools:layout_editor_absoluteX="56dp" />
</RelativeLayout>

</android.support.constraint.ConstraintLayout>
